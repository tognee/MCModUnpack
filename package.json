{
  "name": "MCModUnpack",
  "version": "1.0.0",
  "description": "A modpack installer for minecraft",
  "main": "main.js",
  "author": "tognee <tognee64@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "electron . --dev",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "fs-extra": "^9.1.0",
    "got": "^11.8.2",
    "mime-types": "^2.1.30"
  },
  "build": {
    "appId": "me.tognee.mcmodunpack",
    "productName": "mc-modunpack",
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": "appimage"
    }
  }
}
